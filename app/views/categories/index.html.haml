#content.container.uui
  .grid.rows
    .category-slick
      - @categories.each do |category|
        %div
          = link_to category_path(category) do
            %h1#formatHeader= category.title
          = link_to category_path(category) do
            .banner{:id => "#{category.snap_shot}"}
